{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "Build Project",
		"type": "shell",
		"command": "dotnet build",
		"group": {
		  "kind": "build",
		  "isDefault": true
		},
		"problemMatcher": ["$msCompile"]
	  },
	  {
		"label": "Publish Project",
		"type": "shell",
		"command": "dotnet publish -c Release -o ./publish",
		"group": "build",
		"problemMatcher": []
	  }
	]
  }
  